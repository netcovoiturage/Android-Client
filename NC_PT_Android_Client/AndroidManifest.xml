<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nc_project_team.nc_prototypeinterface"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="18" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="ru.ododo.activities.MainActivity"
            android:label="@string/name_reg_activity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER"
                    />
            </intent-filter>
        </activity>
        <activity android:name="ru.ododo.activities.ListOfRoutes"
                  ></activity>
        <activity android:name="ru.ododo.activities.NameOfNewRoute" android:theme="@android:style/Theme.Dialog">
        </activity>
        <activity android:name="ru.ododo.activities.StartAndDestination"></activity>

        <activity android:name="ru.ododo.activities.SocialNetworkAuth"></activity>
        <activity android:name="ru.ododo.logic.EnterBySocialNetwork"></activity>
    </application>

</manifest>
